/* components/search/search.wxss */
.contain{
  width: 100%;
  background-color: #6eb2f7;
  margin: 0 auto;
  display: flex;
  height: 50px;
  display: flex;
  justify-content: center;
  align-items: center;
}
.search_bar
{
  width: 280px;
  background-color:#f7f8fa;
  height: 35px;
}

.search_icon{
    width: 15px;
    height: 15px;

}

.icon{
    height: 35px;
    width: 40px;
    background-color:#f7f8fa;
    display: flex;
    justify-content: center;
    align-items: center;
    /* border: 1px solid red; */
}

.clear_icon{
    width: 13px;
    height: 13px;
    float: left;
}
