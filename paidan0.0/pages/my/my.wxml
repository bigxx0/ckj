<view>
    <van-cell title="{{calendar_info}}" value="{{ date }}" bind:click="onDisplay" />
<van-tabs>
  <van-tab title="待接单">
        <!-- 这里是待接单的tab页面 -->
        <navigator class="item border-gray" url="/pages/create/create">
            <view class="describe">
                <text>单元格</text>
                <text>描述信息</text>
            </view>
            <text class="content">内容</text>
        </navigator>
        
  
  </van-tab>


  <van-tab title="未完成" >
    <!-- 这里是未完成的tab页面 -->
    <navigator class="item border-gray" url="#">
            <view class="describe">
                <text>单元格</text>
                <text>描述信息</text>
            </view>
            <text class="content">内容</text>
        </navigator>
  </van-tab>


  <van-tab title="已完成">
    <!-- 这里是已完成的tab页面 -->
    
  </van-tab>
</van-tabs>
</view>


<van-calendar
  show="{{ show }}"
  min-date="{{ minDate }}"
  max-date="{{ maxDate }}"
  type="range"
  bind:close="onClose"
  bind:confirm = "onConfirm"
  show-confirm="{{ false }}"
/>