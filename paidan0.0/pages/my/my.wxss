/* pages/my/my.wxss */

.item
{
    /* border-bottom: 1px solid; */
    display: flex;
    justify-content: space-between;
    height: 55px;
    align-items: center;
}
.border-gray
{
    background-color: #f1f1f1;
}
.border-white
{
    background-color: #fff;
}
.describe
{
    display: flex;
    flex-direction: column;
    margin-left: 16px;
}
.describe > text:nth-child(1)
{
    font-size: 14px;
    color: #323233;
}
.describe> text:nth-child(2)
{
    font-size: 12px;
    color: #969799;
}
.content
{
    color: #969799;
    font-size: 14px;
    margin-right: 15px;
}